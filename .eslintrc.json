{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            //"error",
            //2
            "off"
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            //"error",
            //"single"
            "off"
            
        ],
        "semi": [
            //"error",
            //"always"
            "off"
        ],
        "no-console":"off",
        "no-unused-vars":"off"
    }
  
    
    
    
    
    
    
}
